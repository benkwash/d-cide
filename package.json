	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "npm run cy:run & react-scripts test --watchAll=false --forceExit",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"preinstall": "npx npm-force-resolutions",
