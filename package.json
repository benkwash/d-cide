	"jest": {
		"coveragePathIgnorePatterns": [
			"<rootDir>/build/",
			"<rootDir>/node_modules/"
		]
	},
	"resolutions": {
		"yargs-parser": "^18.1.3",
		"http-proxy": "^1.18.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"prettier --write",
			"eslint --fix"
		],
		"*.{html,css,less,ejs,json}": [
			"prettier --write"
		]
	}
